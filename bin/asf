#!/usr/bin/env node

var program = require('commander');

program
    .version('Version 0.1', '-v, --version')
    .version('-i', '--install', 'Install ASF')
    .version('-s', '--start', 'Start the ASF server')
    .parse(process.argv);

if (program.install) {
	console.log('Beginning install of ASF');
}

if (program.start) {
	
}